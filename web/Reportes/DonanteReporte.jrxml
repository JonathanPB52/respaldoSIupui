	<parameter name="TipoDeTelefono" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     donante.`Id` AS donante_Id,
     donante.`nombre` AS donante_nombre,
     donante.`segundoNombre` AS donante_segundoNombre,
     donante.`apellido` AS donante_apellido,
     donante.`segundoApellido` AS donante_segundoApellido,
     donante.`telefono` AS donante_telefono,
     tipotelefono.`tipoDeTelefono` AS tipotelefono_tipoDeTelefono
FROM
     `tipotelefono` tipotelefono INNER JOIN `donante` donante ON tipotelefono.`id` = donante.`tipoTelefono`
WHERE tipotelefono.`tipoDeTelefono`=$P{TipoDeTelefono}]]>
	</queryString>
